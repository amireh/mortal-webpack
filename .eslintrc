{
  "env": {
    "browser": false,
    "node": true
  },
  "parserOptions": {
    "sourceType": "script",
    "ecmaVersion": 6
  },
  "rules": {
    "no-trailing-spaces": 2,
    "indent": ["error", 2, {
      "SwitchCase": 1,
      "ArrayExpression": "first",
      "ObjectExpression": "first"
    }],
    "curly": 0,
    "eqeqeq": 2,
    "quotes": 0,
    "strict": 0,

    "consistent-this": 0,
    "no-undef": 2,
    "dot-notation": 0,
    "space-infix-ops": 0,
    "space-unary-ops": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 2,
    "comma-dangle": 0,
    "semi-spacing": 0,
    "consistent-return": 2,
    "camelcase": 0,
    "eol-last": 0,
    "no-multi-spaces": 0,
    "no-extra-boolean-cast": 0,
    "no-use-before-define": 0,
    "no-extra-bind": 0,
    "no-eq-null": 0,
    "no-shadow": 0,
    "no-shadow-restricted-names": 0,
    "no-alert": 0,
    "no-const-assign": 2,

    "no-unused-expressions": 2,

    "no-console": 0,
    "space-return-throw-case": 0,

    "one-var": [2, {
      "initialized": "never"
    }],

    "no-cond-assign": [2, "except-parens"],
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,

    "valid-jsdoc": [2, {
      "prefer": {
        "returns": "return"
      },

      "requireReturn": false,
      "requireParamDescription": false,
      "requireReturnDescription": false,
    }],

    "no-caller": 2,

    "no-else-return": 0,

    "no-eval": 2,
    "no-implied-eval": 2,
    "no-extend-native": 2,
    "no-fallthrough": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,

    "no-new": 2,
    "no-redeclare": 2,
    "no-return-assign": [2, "always"],
    "no-sequences": 2,
    "radix": 2,
    "wrap-iife": [2, "any"],
    "no-undef-init": 2,
    "no-delete-var": 2,
  },
}
